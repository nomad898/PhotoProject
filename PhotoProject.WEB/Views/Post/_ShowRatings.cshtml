@model ICollection<PhotoProject.WEB.Models.RatingViewModel>

<div id="ratings">
    <br />
    <h2 class="text-field"> Ratings</h2>

    @if (Model.Count() == 0)
    {
        <h6> There are no rating</h6>
    }
    else
    {
        foreach (var item in Model)
        {
            <div class="rating-node">
                <h4 class="text-field">@item.UserName</h4>
                <p class="text-field">@item.RatingValue</p>                
            </div> 
        }
    }
</div>